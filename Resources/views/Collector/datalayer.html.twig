{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
    <span class="icon"><img src="data:image/png;base64," alt=""></span>
    <span class="sf-toolbar-status">300 events</span>
    {% endset %}

    {% set text %}
    <div class="sf-toolbar-info-piece">
        <b>Values</b>
        <span>{{ collector.values | length }}</span>
    </div>
    <div class="sf-toolbar-info-piece">
        <b>Unique events</b>
        <span>300</span>
    </div>
    {% endset %}

    {{ include('@WebProfiler/Profiler/toolbar_item.html.twig', { 'link': true }) }}

{% endblock %}

{% block menu %}
    {# This left-hand menu appears when using the full-screen profiler. #}
    <span class="label">
        <span class="icon"><img src="data:image/png;base64," alt=""/></span>
        <strong>TagCommander</strong>
    </span>
{% endblock %}

{% block panel %}

    <h2>DataLayer</h2>
    <table>
        <thead>
            <tr>
                <th>key</th>
                <th>value</th>
            </tr>
        </thead>
        <tbody>
            {% for key, value in collector.values %}
            <tr>
                <th>{{ key }}</th>

                <td>
                {% if value is iterable %}
                <table>
                {% for subkey, subvalue in value %}
                  <tr>
                    <td>{{ subkey }}</td>
                    <td>{{ subvalue }}</td>
                  </tr>
                {% endfor %}
                </table>
                {% else %}{{ value }}{% endif %}
                </td>
            </tr>
            {% endfor %}
        </tbody>
    </table>

    <h2>Containers</h2>


{% endblock %}